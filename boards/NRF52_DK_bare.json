{
    "name": "NRF52 DK NRF52832",
    "url": "https://www.HashDefineElectronics.com/",
    "vendor": "Nordic",
    "frameworks": [
      "azul"
    ],
    "debug": {
      "default_tools": [
        "jlink"
      ],
      "jlink_device": "nRF52832_xxAA",
      "svd_path": "nrf52.svd"
    },
    "connectivity": [
      "bluetooth"
    ],
    "upload": {
      "maximum_ram_size": 65536,
      "maximum_size": 524288,
      "protocol": "jlink",
      "protocols": [
        "jlink",
        "nrfjprog"
      ]
    },
    "build": {
      "extra_flags": "-DNRF52832_XXAA",
      "cpu": "cortex-m4",
      "f_cpu": "64000000L",
      "mcu": "nrf52832",
      "device": "NRF52832",
      "startup_file": "gcc_startup_nrf52.S",
      "linker_file": "nrf52.ld",
      "includeheaderPaths": [
        "Include/Devices"
      ],
      "IncludeSource" : [
        "Source/system_nrf52.c" 
      ],
      "compiler_flags" : {
        "CCFLAGS" : [
          "-mabi=aapcs",
          "-mfloat-abi=hard",
          "-mfpu=fpv4-sp-d16",
          "-ffunction-sections",
          "-fdata-sections",
          "-fno-strict-aliasing",
          "-fno-builtin",
          "-fshort-enums"
        ],
        "LINKFLAGS" : [
          "-mfloat-abi=hard",
          "-mfpu=fpv4-sp-d16"
        ]
      }
    }
  }